type (
	LoginReq {
		Name     string `json:"name"`
		Password string `json:"password"`
	}

	LoginReply {
		Id       int64  `json:"id"`
		Name     string `json:"name"`
		Phone    string `json:"phone"`
		Password string `json:"password"`
	}
)

service user-api {
	@handler login
	post /user/login (LoginReq) returns (LoginReply)
}